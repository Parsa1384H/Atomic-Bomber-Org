<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ChangePasswordControllerView">
    <children>
        <TextField layoutX="226.0" layoutY="164.0"/>
        <PasswordField layoutX="226.0" layoutY="209.0"/>
        <PasswordField layoutX="226.0" layoutY="253.0"/>
        <Label alignment="CENTER" fx:id="currentPassword" layoutX="91.0" layoutY="168.0" prefHeight="17.0"
               prefWidth="115.0"
               text="Current Password :" style="-fx-background-color: white;"/>
        <Label alignment="CENTER" fx:id="newPassword" layoutX="91.0" layoutY="213.0" prefHeight="17.0" prefWidth="115.0"
               text="New Password :" style="-fx-background-color: white;"/>
        <Label alignment="CENTER" fx:id="confirmPassword" layoutX="91.0" layoutY="257.0" prefHeight="17.0"
               prefWidth="115.0" text="Confirm Password :" style="-fx-background-color: white;"/>
        <Button layoutX="226.0" layoutY="298.0" mnemonicParsing="false" text="Change" onMouseClicked="#changePassword"/>
        <Button layoutX="323.0" layoutY="298.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="57.0" text="Back"
                onMouseClicked="#back"/>
    </children>
</AnchorPane>
